{
  "name": "chisu/php-integration",
  "description": "chisu php integration for TB phase II.",
  "keywords": [
    "chisu",
    "php-integration"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "path",
      "url": "../satusehat-mediator-client-php"
    }
  ],
  "require": {
    "php": "^7.4|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/config": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/session": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/validation": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/translation": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/routing": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/events": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/view": "^8.0|^9.0|^10.0|^11.0",
    "kemenkesri/satusehat-mediator-client-php": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "laravel/tinker": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "Chisu\\PhpIntegration\\": "src/"
    },
    "files": [
      "helpers.php",
      "Core/manual-integration.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
